{
    "name": "beaten-sect0r/yii2-flatpickr",
    "description": "Datetime picker widget for Yii2 framework",
    "keywords": ["yii2", "extension", "datetime picker", "flatpickr"],
    "authors": [
        {
            "name": "Alexander Volkov",
            "email": "beatensect0r@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/Beaten-Sect0r/yii2-flatpickr/issues?state=open",
        "source": "https://github.com/Beaten-Sect0r/yii2-flatpickr"
    },
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/flatpickr-calendar": ">=3"
    },
    "autoload": {
        "psr-4": {
            "bs\\Flatpickr\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
